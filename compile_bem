#PBS -S /bin/bash
#PBS -q main
#PBS -l walltime=00:10:00

#PBS -l select=1:ncpus=1:mem=6GB

module load mkl
module load gcc/7.3.0

cd $PBS_O_WORKDIR

ITENSOR_V3_PATH='/home/marmac96/itensor'

g++ main.cpp -o kondo_heisenberg_model -m64 -std=c++17 -fconcepts -fPIC -I '/home/marmac96/itensor' -O2 -DNDEBUG -Wall -Wno-unknown-pragmas -Wno-unused-variable -L'/home/marmac96/itensor/lib' -litensor -L/usr/lib -lblas -llapack -lpthread -lm -lc
